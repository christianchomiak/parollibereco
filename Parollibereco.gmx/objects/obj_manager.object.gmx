table_height = 1 * room_height;

person_width = table_width / columns;
person_height = table_height / rows;

avatar_height = 0.5 * person_height;
bubble_height = 0.5 * person_height;
speech_height = 0.4 * person_height;

//show_message("W: " + string(person_width) + "H: " + string(person_height) );

